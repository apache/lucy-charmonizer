#define CHAZ_USE_SHORT_NAMES

#include "_charm_test.h"
#include <string.h>
#include "Charmonizer/Test/TestHandler.h"

TestBatch*
chaz_TestHand_test_FuncMacro()
{
    TestBatch *batch = TestHand_new_batch(3);

#ifdef HAS_FUNC_MACRO
    ASSERT_TRUE(batch, 
        (strcmp(Func_Macro, "chaz_TestHand_test_FuncMacro") == 0), 
        "Func_Macro");
#endif

#ifdef HAS_ISO_FUNC_MACRO
    ASSERT_TRUE(batch, 
        (strcmp(__func__, "chaz_TestHand_test_FuncMacro") == 0),
        "HAS_ISO_FUNC_MACRO");
#endif

#ifdef HAS_GNUC_FUNC_MACRO
    ASSERT_TRUE(batch, 
        (strcmp(__FUNCTION__, "chaz_TestHand_test_FuncMacro") == 0), 
        "HAS_GNUC_FUNC_MACRO");
#endif
    
    return batch;
}

