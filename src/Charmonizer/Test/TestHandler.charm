#define CHAZ_USE_SHORT_NAMES

#include <stdio.h>
#include "Charmonizer/Test/TestHandler.h"

chaz_bool_t
chaz_TestHand_report(int value, const char *message, int test_num)
{
    if (value) {
        printf("%-4d pass: %s\n", test_num, message);
        return true;
    }
    else {
        printf("%-4d fail: %s\n", test_num, message);
        return false;
    }
}

